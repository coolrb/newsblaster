turn result;\n  }\n\n\n  if (hasTag(\'vis-dispatch\') && isDesktop()) {\n\n    function createDispatchByline() {\n      var fallbackImage = \'https://static01.nyt.com/newsgraphics/2016/09/13/visual-pilot-assets/assets/transparent.gif\';\n\n      var headline       = document.querySelector(\'h1#headline\');\n      var dispatchTitle  = document.querySelector(\'.story-notes p strong\') ? document.querySelector(\'.story-notes p strong\').textContent : \'\';\n      var dispatchAuthor = document.querySelector(\'.story-notes p\').textContent.replace(dispatchTitle, \'\');\n      var dispatchImage  = getPortraitFromString(dispatchAuthor);//\'https://static01.nyt.com/newsgraphics/2016/08/15/vis-pilot/byline-anne-barnard.png\';\n\n      var dispatchByline = document.createElement(\'div\');\n      dispatchByline.className = \'g-dispatch-byline\';\n      \n      // Incorporate onerror to replace any broken images with a generic image. Just a safeguard\n      // <img src="foo.jpg" onerror="if (this.src != \'error.jpg\') this.src = \'error.jpg\';">\n      dispatchByline.innerHTML = \'<div class="g-dispatch-headshot"><img src="\' + dispatchImage + \'" onerror="if (this.src != \\\'\'+fallbackImage+\'\\\') this.src = \\\'\'+fallbackImage+\'\\\'"></div>\' +\n                                      \'<h3>\' + dispatchTitle + \'</h3>\' +\n                                      \'<h5>\' + dispatchAuthor + \'</h5>\';\n\n      headline.parentNode.insertBefore(dispatchByline, headline);\n    }\n\n    var interval = setInterval(function() {\n      if (document.querySelector(\'.story-notes p\')) {\n        createDispatchByline();\n        clearInterval(interval);\n      }\n    }, 50);\n\n  }\n\n})();</script>\n\n<!-- <div id="g-graphic" data-preview-slug="2016-09-13-visual-pilot-assets"></div> --><!-- Pipeline: 2016-09-13-visual-pilot-assets August 3, 2017, 11:45AM 76ed315589ace8bd9f362b00c2e93ce02a957e32 --></div>\n\n        <div id="story-meta" class="story-meta ">\n                           