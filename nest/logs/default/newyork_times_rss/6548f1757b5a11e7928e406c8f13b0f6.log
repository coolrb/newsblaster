adata (`tags.desktop` and `tags.mobile`,\n    // loop over them and grab any we are interested in (Those which show up in `targetTags`)\n    var tagsFound = checkForTargetTags(tags, allTags, targetTags);\n\n    // add the classes\n    // also adds some generic classes based on whether we saw mobile or desktop metadata.\n    addClasses(tagsFound);\n\n    return tagsFound;\n  }\n\n\n\n  // inject a survey box at the end\n  function addSurveyBox() {\n    // remove crud at the end of the current url, hopefully standardizing it for surveys\n    var loc = getPathFromUrl(window.location.href);\n\n    var blurbText = \'We\\\'re interested in your feedback on this page. <strong> Tell us what you think.</strong>\';\n    var blurbUrl  = \'http://nyt.qualtrics.com/jfe/form/SV_4INloKlK2qDNyfz?source=\'+loc;\n\n\n    var story     = isDesktop() ? document.querySelector(\'#story\') : document.querySelector(\'.article-page article\');\n    var surveyBox = document.createElement(\'div\');\n\n    \n    surveyBox.className = \'vis-survey-box\';\n    surveyBox.innerHTML = \'<a href="\'+blurbUrl+\'">\'+blurbText+\'</a>\';\n\n    story.parentNode.insertBefore(surveyBox, story.nextSibling);\n  }\n\n\n  // per-template desktop scripts\n\n  function desktopScripts_dispatch() {\n    function createDispatchByline() {\n      var fallbackImage = \'https://static01.nyt.com/newsgraphics/2016/09/13/visual-pilot-assets/assets/transparent.gif\';\n\n      var headline       = document.querySelector(\'h1#headline\');\n      var dispatchTitle  = document.querySelector(\'.story-notes p strong\').textContent;\n      var dispatchAuthor = document.querySelector(\'.story-notes p\').textContent.replace(dispatchTitle, \'\');\n      var dispatchImage  = getPortraitFromString(dispatchAuthor);//\'https://static01.nyt.com/newsgraphics/2016/08/15/vis-pilot/byline-anne-barnard.png\';\n\n      var dispatchByline = document.createElement(\'div\');\n      dispatchByline.className = \'g-dispatch-byline\';\n      \n    